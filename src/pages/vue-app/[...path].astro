import App from '../../vue-app/App.vue';

export function getStaticPaths() {
	const vueRoutes = ['about', 'blog', 'test', 'contact']

	const staticPaths =  vueRoutes.map((route) => (
		{params: {path: route}}
	))

	return [
		...staticPaths,
		{ params: { id: undefined}},
	];
}

